{
    "manifest_version": 2,
    "name": "screen-sharing",
    "version": "0.10",
    "background": {
      "scripts": ["extension.js"]
    },
    "externally_connectable": {
      "matches": ["*://localhost/*", "*://*.example.com/*"]
    },
    "permissions": [
      "desktopCapture",
      "tabs"
    ]
  }